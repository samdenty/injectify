{
  "name": "injectify-core",
  "version": "1.0.0",
  "description": "The injectify Core & modules API",
  "main": "core.ts",
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --colors --config ./webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack -p --progress --colors --config ./webpack.config.js",
    "build:silent": "cross-env NODE_ENV=production webpack -p --colors --config ./webpack.config.js",
    "install:all": "yarn-recursive --cmd install",
    "dev": "yarn run watch"
  },
  "repository": "https://github.com/samdenty99/injectify",
  "author": "samdenty99",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "circular-json": "^0.5.1",
    "color": "^3.0.0",
    "cross-env": "^5.1.3",
    "crypto-js": "^3.1.9-1",
    "glob": "^7.1.2",
    "guid": "^0.0.12",
    "jss": "^9.7.0",
    "jss-preset-default": "^4.2.0",
    "pako": "^1.0.6",
    "pretty-ms": "^3.1.0",
    "promise-polyfill": "^7.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "typescript": "^2.7.1",
    "unminified-webpack-plugin": "^1.4.2",
    "webpack": "^3.10.0",
    "yarn-recursive": "^1.0.5"
  }
}
