{
  "name": "injectify-core",
  "version": "1.0.0",
  "description": "The injectify Core & modules API",
  "main": "core.ts",
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --colors --config ./webpack/debug.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config ./webpack/production.config.js && cross-env NODE_ENV=production webpack --progress --colors --config ./webpack/debug.config.js",
    "build:silent": "cross-env NODE_ENV=production webpack --colors --config ./webpack/production.config.js && cross-env NODE_ENV=production webpack --colors --config ./webpack/debug.config.js",
    "install:all": "yarn-recursive --cmd install",
    "dev": "yarn run watch"
  },
  "repository": "https://github.com/samdenty99/injectify",
  "author": "samdenty99",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^5.0.0-1",
    "byte-length": "^1.0.2",
    "chalk": "^2.3.2",
    "circular-json": "^0.5.1",
    "color": "^3.0.0",
    "cross-env": "^5.1.3",
    "glob": "^7.1.2",
    "guid": "^0.0.12",
    "ifdef-loader": "^2.0.3",
    "jss": "^9.7.0",
    "jss-preset-default": "^4.2.0",
    "pako": "^1.0.6",
    "pretty-bytes": "^4.0.2",
    "pretty-ms": "^3.1.0",
    "promise-polyfill": "^7.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "yarn-recursive": "^1.0.5"
  }
}
